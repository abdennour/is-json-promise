{
  "name": "is-json-promise",
  "version": "1.0.1",
  "description": "Check if string is JSON basing on PROMISE instead of TRY-CATCH",
  "main": "index.js",
  "scripts": {
    "compile": "babel --presets es2015,stage-0 -d lib/ src/",
    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "test": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- test/*.js",
    "coverage": "istanbul cover _mocha -- --ui bdd -R spec -t 5000;open ./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdennour/is-json-promise.git"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-istanbul": "^0.11.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "expect": "^1.20.2",
    "mocha": "^3.1.2",
    "sinon": "^1.17.6"
  },
  "keywords": [
    "is-json",
    "string",
    "json",
    "promise",
    "then",
    "babel",
    "ES7"
  ],
  "author": "Abdennour TOUMI <http://abdennoor.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abdennour/is-json-promise/issues"
  },
  "homepage": "https://github.com/abdennour/is-json-promise#readme"
}
